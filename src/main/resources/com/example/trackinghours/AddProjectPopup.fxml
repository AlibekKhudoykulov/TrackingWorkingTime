<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.trackinghours.controller.AddProjectPopupController"
            prefHeight="150.0" prefWidth="300.0">

    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Project Name:" />
        <Label layoutX="14.0" layoutY="54.0" text="Project Number:" />
        <TextField layoutX="110.0" layoutY="10.0" fx:id="projectNameField" />
        <TextField layoutX="110.0" layoutY="50.0" fx:id="projectNumberField" />
        <Button layoutX="129.0" layoutY="84.0" mnemonicParsing="false" text="Add" onAction="#addProject" />
    </children>
</AnchorPane>
